{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/(.*)\\.mpd",
      "dest": "/api/index.php?id=$1&$query",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/(.*)\\.m3u8",
      "dest": "/api/index.php?id=$1",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/aiimg/(.*)\\.img",
      "dest": "/api/aiimg.php?url=$1",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/",
      "dest": "/api/index.php",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/style.css",
      "dest": "/api/style.css",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/videos.json",
      "dest": "/api/videos.json",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/create-json.php",
      "dest": "/api/create-json.php",
      "headers": {
        "Cache-Control": "max-age=20, public"
      }
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/api/$1.png",
      "headers": {
        "Cache-Control": "max-age=20000, public"
      }
    }
  ],
  "regions": ["bom1"]
}





